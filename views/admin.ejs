<!-- this script tag is brought to you by https://www.w3schools.com/howto/howto_js_draggable.asp -->
<script>
  //Make the DIV element draggagle:
  dragElement(document.getElementById("draggable"));
  
  function dragElement(elmnt) {
    console.log("11")
    var pos1 = 0, pos2 = 0, pos3 = 0, pos4 = 0;
    if (document.getElementById(elmnt.id)) {
      /* if present, the header is where you move the DIV from:*/
      document.getElementById(elmnt.id).onmousedown = dragMouseDown;
    } else {
      /* otherwise, move the DIV from anywhere inside the DIV:*/
      elmnt.onmousedown = dragMouseDown;
    }
  
    function dragMouseDown(e) {
      console.log("22")
      e = e || window.event;
      e.preventDefault();
      // get the mouse cursor position at startup:
      pos3 = e.clientX;
      pos4 = e.clientY;
      document.onmouseup = closeDragElement;
      // call a function whenever the cursor moves:
      document.onmousemove = elementDrag;
    }
  
    function elementDrag(e) {
      console.log("33")
      e = e || window.event;
      e.preventDefault();
      // calculate the new cursor position:
      pos1 = pos3 - e.clientX;
      pos2 = pos4 - e.clientY;
      pos3 = e.clientX;
      pos4 = e.clientY;
      // set the element's new position:
      elmnt.style.top = (elmnt.offsetTop - pos2) + "px";
      elmnt.style.left = (elmnt.offsetLeft - pos1) + "px";
    }
  
    function closeDragElement() {
      console.log("44")
      /* stop moving when mouse button is released:*/
      document.onmouseup = null;
      document.onmousemove = null;
    }
  }





  ontouchmoveElement(document.getElementById("draggable"));
  function ontouchmoveElement(elmnt) {
    console.log("1")
    var pos1 = 0, pos2 = 0, pos3 = 0, pos4 = 0;
    if (document.getElementById(elmnt.id)) {
      /* if present, the header is where you move the DIV from:*/
      document.getElementById(elmnt.id).ontouchstart = ontouchmoveMobile;
    } else {
      /* otherwise, move the DIV from anywhere inside the DIV:*/
      elmnt.ontouchstart = ontouchmoveMobile;
    }
  
    function ontouchmoveMobile(e) {
      console.log("2")
      e = e || window.event;
      // get the mouse cursor position at startup:
      pos3 = e.touches[0].clientX;
      pos4 = e.touches[0].clientY;
      document.ontouchend = closeDragElementMobile;
      // call a function whenever the cursor moves:
      document.ontouchmove = elementDragMoblie;
    }
  
    function elementDragMoblie(e) {
      console.log("3")
      e = e || window.event;
      // calculate the new cursor position:
      pos1 = pos3 - e.touches[0].clientX;
      pos2 = pos4 - e.touches[0].clientY;
      pos3 = e.touches[0].clientX;
      pos4 = e.touches[0].clientY;
      // set the element's new position:
      elmnt.style.top = (elmnt.offsetTop - pos2) + "px";
      elmnt.style.left = (elmnt.offsetLeft - pos1) + "px";
    }
  
    function closeDragElementMobile() {
      console.log("4")
      /* stop moving when mouse button is released:*/
      document.ontouchend = null;
      document.ontouchstart = null;
    }
  }
</script>


