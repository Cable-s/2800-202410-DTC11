<style>
    .slidecontainer {
      width: 100%;
    }
    
    .slider {
      -webkit-appearance: none;
      width: 100%;
      height: 10px;
      border-radius: 0.375rem;
      background: #FFFFFF   ;
      outline: none;
      -webkit-transition: .2s;
      transition: opacity .2s;
    }
    
    .slider::-webkit-slider-thumb {
      -webkit-appearance: none;
      appearance: none;
      border-color: #222229;
      border-width: 2px;
      border-radius: 9999px;
      width: 25px;
      height: 25px;
      background: #B2EBF2;
      cursor: pointer;
    }
    
    .slider::-moz-range-thumb {
      width: 25px;
      height: 25px;
      background: #04AA6D;
      cursor: pointer;
    }
    </style>
    <!-- END of slider CSS -->
  
  <!-- our own scripts -->
  <script src="https://cdn.tailwindcss.com"></script>
  <script>
    tailwind.config = {
        theme: {
            extend: {
                colors: {
                    primary: '#2D2B32',
                    secondary: '#B2EBF2',
                    background: '#222229',
                    accent: 'white'
                }
            }
        }
    }
    </script>

<div class="slidecontainer">  
    <input type="range" min="1" max="100" value="50" class="slider" id="myRange">
</div>